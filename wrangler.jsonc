{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "holmdahl-io",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2025-06-16",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "route": {
    "zone_name": "holmdahl.io",
    "pattern": "holmdahl.io/*"
  },
  "observability": {
    "enabled": true
  },
  "env": {
    "preview": {
      "route": {
        "zone_name": "holmdahl.io",
        "pattern": "preview.holmdahl.io/*"
      }
    }
  }
}