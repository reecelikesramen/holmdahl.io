{
  "private": true,
  "name": "holmdahl.io",
  "type": "module",
  "scripts": {
    "dev": "concurrently 'vite' 'hugo server'",
    "build": "hugo --baseURL '/' && vite build && shx cp ./serve.json ./dist/serve.json",
    "clean": "rm -rf public && rm -rf dist",
    "serve": "serve dist"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "glob": "^11.0.0",
    "hugo-bin": "^0.136.3",
    "serve": "^14.2.4",
    "shx": "^0.3.4",
    "vite": "^5.4.11",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.3.0"
  },
  "dependencies": {
    "wasm-feature-detect": "^1.8.0"
  }
}
